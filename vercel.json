{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/send-ticket",
            "destination": "/api/send-ticket.cjs"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}